# Ignore node modules (always)
node_modules/

# Ignore Webpack build output (e.g., "dist" or "build")
dist/
build/

# Ignore Webpack cache (improves rebuild times but shouldn't be committed)
.cache/
webpack.cache/

# Ignore log files
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore environment files (security reasons)
.env
.env.local